<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="at.bestsolution.efxclipse.runtime.workbench.application"
            name="%product.name">
         <property
               name="windowImages"
               value="icons/alt_window_16.gif,icons/alt_window_32.gif">
         </property>
         <property
               name="applicationXMI"
               value="at.bestsolution.efxclipse.runtime.examples.e4/Application.e4xmi">
         </property>
         <property
               name="appName"
               value="Hello JavaFX e4">
         </property>
      </product>
   </extension>
   <extension
         point="at.bestsolution.efxclipse.runtime.theme">
      <theme
            basestylesheet="css/default.css"
            id="at.bestsolution.efxclipse.runtime.examples.e4.default">
      </theme>
   </extension>

</plugin>
