        eType="#//CssTok" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="css_not">
    <eLiterals name="not" literal="not("/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CssTok"/>
