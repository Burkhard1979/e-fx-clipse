<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="id1"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="any"
            visible="true">
         <run
               class="at.bestsolution.efxclipse.runtime.examples.contacts.app.ContactsOSGiApplication">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="at.bestsolution.efxclipse.runtime.examples.contacts.app.id1"
            name="contactssample">
         <property
               name="appName"
               value="contactssample">
         </property>
      </product>
   </extension>

</plugin>
